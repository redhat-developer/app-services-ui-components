{
  "name": "@rhoas/app-services-ui-components",
  "version": "0.0.0",
  "description": "UI Shared contains a number of type definitions for shared data structures and components for the application services UI.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "postbuild": "npm run copy-files",
    "copy-files": "npm run copy-files-esm && npm run copy-files-cjs",
    "copy-files-esm": "copyfiles -u 1 src/**/*.css dist/esm",
    "copy-files-cjs": "copyfiles -u 1 src/**/*.css dist/cjs",
    "package": "npm run build",
    "prepare": "npm run clean && npm run package",
    "clean": "rimraf dist",
    "lint": "npm run eslint",
    "eslint": "eslint --ext .tsx,.js,.ts ./src/",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "chromatic"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redhat-developer/app-services-ui-components.git"
  },
  "keywords": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/redhat-developer/app-services-ui-components/issues"
  },
  "homepage": "https://github.com/redhat-developer/app-services-ui-components#readme",
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@storybook/addon-actions": "6.4.9",
    "@storybook/addon-a11y": "6.4.9",
    "@storybook/addon-docs": "6.4.9",
    "@storybook/addon-essentials": "6.4.9",
    "@storybook/addon-interactions": "6.4.9",
    "@storybook/addon-links": "6.4.9",
    "@storybook/jest": "0.0.5",
    "@storybook/react": "6.4.9",
    "@storybook/testing-library": "0.0.7",
    "@types/byte-size": "8.1.0",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@types/react-router-dom": "5.3.2",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "@xstate/inspect": "0.5.2",
    "babel-loader": "8.2.3",
    "chromatic": "6.2.0",
    "copyfiles": "2.4.1",
    "dayjs": "1.10.7",
    "eslint": "7.32.0",
    "i18next-pseudo": "2.2.1",
    "prando": "6.0.1",
    "rimraf": "3.0.2",
    "storybook-i18n": "1.0.6",
    "typescript": "4.5.3"
  },
  "dependencies": {
    "@patternfly/patternfly": "4.132.2",
    "@patternfly/react-charts": "6.28.4",
    "@patternfly/react-core": "4.152.4",
    "@patternfly/react-icons": "4.11.14",
    "@patternfly/react-table": "4.29.58",
    "@rhoas/app-services-ui-shared": "0.15.2",
    "@xstate/react": "1.6.3",
    "byte-size": "8.1.0",
    "date-fns": "2.27.0",
    "date-fns-tz": "1.1.6",
    "i18next": "21.6.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-i18next": "11.15.1",
    "react-router-dom": "5.2.1",
    "tslib": "2.3.1",
    "xstate": "4.26.1"
  }
}
